[alias]
    co = checkout
    st = status
    ci = commit
    br = branch
    cl = clean -xdf
    re = reset --hard
    up = submodule update --recursive --remote
    dl = !git branch | grep -v master | xargs git branch -D
    lo = log --decorate --oneline --graph
    ir1 = rebase -i HEAD~1
    ir2 = rebase -i HEAD~2
    ir3 = rebase -i HEAD~3
    ir4 = rebase -i HEAD~4
    ir5 = rebase -i HEAD~5
    ir6 = rebase -i HEAD~6
    ir7 = rebase -i HEAD~7
    ir8 = rebase -i HEAD~8
    ir9 = rebase -i HEAD~9
    ir10 = rebase -i HEAD~10
    d = difftool
[user]
	name = Rudy Rodriguez Lucha
	email = rodriguezlucha@gmail.com
[diff]
    tool = vimdiff

[push]
    default = simple
[core]
	excludesfile = /Users/rudy/.gitignore_global
[color]
	ui = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
